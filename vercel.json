{
  "builds": [
    {
      "src": "testing_algorithms_django/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/media/(.*)",
      "destination": "/results/$1"
    },
    {
      "source": "/(.*)",
      "destination": "testing_algorithms_django/wsgi.py"
    }
  ]
}
